---
import { DefaultSettings, type SupportedLanguage } from '../settings';

export type Props = {
  language: SupportedLanguage
}

const { language } = Astro.props;
---

<header>
  <div class="container relative mx-auto p-4 flex text-slate-500 items-center">
    <h1 class="text-xl font-black antialiased mr-auto">{DefaultSettings[language].title}</h1>
    <!--<PostSearch />-->
  </div>
</header>

